<script>
    $(document).ready(function () {
        const userNsp = io('/userNsp');
        userNsp.emit('connectUser', {
            rid: 'U<%=account.id%>'
        });

        userNsp.on('notifMsg', function (msg) {
            createNewNotif({
                notiftext: msg.sender + ': ' + msg.message + ' in ' + msg.listid
            });
        });
    });
</script>